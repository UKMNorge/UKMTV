RewriteOptions inherit
Options +FollowSymlinks
RewriteEngine on
RewriteBase /

### SØK OPP FIL-INFO BASERT PÅ CRON_ID
RewriteCond %{SCRIPT_FILENAME} !-d  
RewriteCond %{SCRIPT_FILENAME} !-f  
RewriteRule ^finn/([0-9]+)/?$ file_info.php?cron=$1 [L]

### SØK OPP FIL-INFO BASERT PÅ FILBANE
RewriteCond %{SCRIPT_FILENAME} !-d  
RewriteCond %{SCRIPT_FILENAME} !-f  
RewriteRule ^finn/(.+)/?$ file_info.php?file=$1 [L]

### VIS EN KATEGORI-GRUPPE
RewriteCond %{SCRIPT_FILENAME} !-d  
RewriteCond %{SCRIPT_FILENAME} !-f  
RewriteRule ^kategorier/(.+)/?$ index.php?kat=kategorier&folder=$1 [L]

RewriteCond %{SCRIPT_FILENAME} !-d  
RewriteCond %{SCRIPT_FILENAME} !-f  
RewriteRule ^([_0-9a-zA-Z-]+)?$ $1/ [R=301,L]

### SØK EN KATEGORI
RewriteCond %{SCRIPT_FILENAME} !-d  
RewriteCond %{SCRIPT_FILENAME} !-f  
RewriteRule ^samling/(.+)/?$ index.php?kat=kategori&sok=$1 [L]

### KATEGORIER a-å
RewriteCond %{SCRIPT_FILENAME} !-d  
RewriteCond %{SCRIPT_FILENAME} !-f  
RewriteRule ^a-a/(.+)/?$ index.php?kat=a-a&alpha=$1 [L]

### RELATERT
RewriteCond %{SCRIPT_FILENAME} !-d  
RewriteCond %{SCRIPT_FILENAME} !-f  
RewriteRule ^relatert/([a-z_0-9]+)/?$ index.php?kat=relatert&sok=$1 [L]

### VIS EN GITT VIDEO
RewriteCond %{SCRIPT_FILENAME} !-d  
RewriteCond %{SCRIPT_FILENAME} !-f  
RewriteRule ^([_0-9a-zA-Z-]+)/([0-9]+)/?$ index.php?video=$2 [L]

### ANGI KATEGORI
RewriteCond %{SCRIPT_FILENAME} !-d  
RewriteCond %{SCRIPT_FILENAME} !-f  
RewriteRule ^([_0-9a-zA-Z-]+)/$ index.php?kat=$1 [L]

RewriteCond %{SCRIPT_FILENAME} !-d  
RewriteCond %{SCRIPT_FILENAME} !-f  
RewriteRule ^(.*)$ index.php [L]
